type AuthPayload {
  token: String!
  userID: ID!
  username: String!
  name: String!
  role: String!
}

type Mutation {
  register(
    username: String!
    name: String
    email: String
    password: String!
  ): AuthPayload!
  login(
    username: String!
    password: String!
  ): AuthPayload!
}
